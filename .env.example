# RentGenius Agent #3 - Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit the actual .env file to git

# Google Calendar API
GOOGLE_CALENDAR_CREDENTIALS=path/to/your/credentials.json
# Download from: https://console.cloud.google.com/apis/credentials

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
# Get from: https://console.cloud.google.com/apis/api/maps-backend.googleapis.com

# Email Configuration (Gmail App Password)
EMAIL_ADDRESS=your_email@gmail.com
EMAIL_PASSWORD=your_app_password_here
# Generate app password: https://support.google.com/accounts/answer/185833

# TiDB Serverless Connection
TIDB_CONNECTION_STRING=mysql://username:password@gateway01.us-west-2.prod.aws.tidbcloud.com:4000/database_name?ssl=true
# Get from: https://tidbcloud.com/

# Twilio SMS (Optional)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Development Settings
DEBUG_MODE=true
LOG_LEVEL=INFO
DEMO_MODE=false